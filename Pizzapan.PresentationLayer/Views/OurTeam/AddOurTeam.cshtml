@model OurTeam
@{
    ViewData["Title"] = "AddOurTeam";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h1>Kategori Ekleme Sayfası</h1>


<form method="post">

    <label>Personel Adı</label>
    <input asp-for="OurTeamName" class="form-control" style="background-color:#b1b3b1" />
    <br />
    <label>Personel Mevkisi</label>
    <input asp-for="OurTeamStatus" class="form-control" style="background-color:#b1b3b1" />
    <br />
    <button class="btn btn-info">Kaydet</button>
</form>
